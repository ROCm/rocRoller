[
  {
    "selector": "node[label]",
    "style": {
      "label": "data(label)"
    }
  },
  {
    "selector": "edge[label]",
    "style": {
      "label": "data(label)"
    }
  },
  {
    "selector": "edge",
    "style": {
      "curve-style": "bezier",
      "target-arrow-shape": "vee",
      "target-arrow-color": "red",
      "line-color": "red"
    }
  },
  {
    "selector": "node.hg-edge",
    "style": {
      "shape": "triangle"
    }
  },
  {
    "selector": "edge.Body",
    "style": {
      "line-color": "blue",
      "target-arrow-color": "blue"
    }
  },
  {
    "selector": "edge.Sequence",
    "style": {
      "line-color": "green",
      "target-arrow-color": "green"
    }
  }
  ,
  {
    "selector": "node.composite",
    "style": {
      "visibility": "visible"
    }
  },
  {
    "selector": ".target-selected",
    "style": {
      "line-color": "pink",
      "background-color": "pink"
    }
  },
  {
    "selector": ".hidden",
    "style":{
      "visibility": "hidden"
    }

  },
  {
    "selector": ".ordering",
    "style":{
      "visibility": "visible",
      "line-color": "rgb(192, 192, 192)",
      "target-arrow-color": "rgb(192, 192, 192)"
    }
  },
  {
    "selector": "node.subgraph",
    "style": {
      "background-color": "white",
      "events": "yes"
    }
  },

